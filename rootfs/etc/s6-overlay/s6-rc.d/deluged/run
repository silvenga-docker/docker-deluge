#!/command/execlineb -P

importas niceness NICENESS

nice --adjustment=${niceness}

s6-setuidgid ubuntu

/usr/bin/deluged \
    --config /config \
    --ui-interface 0.0.0.0 \
    --interface 0.0.0.0 \
    --loglevel info \
    --logfile /config/deluged.log \
    --pidfile /tmp/deluge.pid \
    --do-not-daemonize
